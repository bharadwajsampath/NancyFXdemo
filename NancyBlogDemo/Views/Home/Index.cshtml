@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<NancyBlogDemo.Models.BlogPost>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table>
        <tr>
            <td>Id:</td>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <td>Title:</td>
            <td>@Model.Title</td>
        </tr>
        <tr>
            <td>Content:</td>
            <td>@Model.Content</td>
        </tr>
        <tr>
            <td>Tags:</td>
            <td>@string.Join(", ", Model.Tags)</td>
        </tr>
    </table>
</body>
</html>
